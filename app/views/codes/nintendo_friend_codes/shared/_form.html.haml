= form_with(model: [nintendo_friend_code], url: form_path, local: true) do |f|
    = render 'shared/error_messages', model: f.object
    .form-group
        = f.label :friend_code
        = f.text_field :friend_code, minlength: 12, maxlength: 12, class: 'form-control'

    .form-group
        = f.submit nil, class:'btn btn-primary'